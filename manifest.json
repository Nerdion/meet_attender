{
  "manifest_version": 2,
  "name": "GMeet Joiner",
  "version": "0.1",
  "description": "This is a Google Meet automatic joiner based on link and time specified",
  "author": "Neel Khalade",
  "icons": {
    "300": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "GMeet Joiner",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["consoleWindow.js"],
      "all_frames": true
    }
  ]
}
